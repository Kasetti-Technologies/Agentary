{
  "name": "agentary",
  "version": "1.0.0",
  "description": "Agentary Express service with middleware, Docker setup, and testing via Jest + Supertest.",
  "main": "index.js",
  "type": "commonjs",
  "directories": {
    "doc": "docs"
  },
"scripts": {
  "billing:dev": "ts-node src/billing-aggregator-main.ts",
  "billing:invoice": "ts-node src/billing-invoice-main.ts",
  "api:dev": "ts-node src/server.ts",
  "api:start": "node dist/server.js",
  "test": "jest --detectOpenHandles",
  "test:watch": "jest --watch",
  "seed": "node ci/scripts/seed-testdata.js",
  "build": "tsc --project tsconfig.json",
  "watch": "tsc --project tsconfig.json --watch"
},

  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kasetti-Technologies/Agentary.git"
  },
  "keywords": [
    "express",
    "typescript",
    "jest",
    "supertest",
    "middleware",
    "jwks"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Kasetti-Technologies/Agentary/issues"
  },
  "homepage": "https://github.com/Kasetti-Technologies/Agentary#readme",
  "dependencies": {
    "@aws-sdk/client-kms": "^3.922.0",
    "@kubernetes/client-node": "^1.4.0",
    "axios": "^1.12.2",
    "express": "^5.1.0",
    "fs": "^0.0.1-security",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.2.0",
    "kafkajs": "^2.2.4",
    "path": "^0.12.7",
    "pg": "^8.16.3",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@redocly/cli": "^2.5.0",
    "@types/axios": "^0.9.36",
    "@types/axios-mock-adapter": "^1.9.0",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/kafkajs": "^1.8.2",
    "@types/node": "^24.9.1",
    "@types/on-finished": "^2.3.5",
    "@types/pg": "^8.15.5",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/supertest": "^6.0.3",
    "axios-mock-adapter": "^2.1.0",
    "install": "^0.13.0",
    "jest": "^30.2.0",
    "jose": "^6.1.0",
    "nock": "^14.0.10",
    "react-scripts": "^5.0.1",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
